     ]
    }
   ],
   "source": [
    "while True:\n",
    "    s = input()\n",
    "    if s == '0':\n",
    "        break\n",
    "    if ''.join(list(reversed(s))) == s:\n",
    "        print('yes')\n",
    "    else:\n",
    "        print('no')"
   ]
