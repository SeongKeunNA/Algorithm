    "n = int(input())\n",
    "if n == 0:\n",
    "    print(0)\n",
    "else:\n",
    "    d = [0] * (n + 1)\n",
    "    d[1] = 1\n",
    "    for i in range(2, n + 1):\n",
    "        d[i] = d[i-2] + d[i-1]\n",
    "    print(d[n])"
