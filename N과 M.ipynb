   ],
   "source": [
    "from itertools import permutations as pm\n",
    "n, m = map(int, input().split())\n",
    "possible = pm(range(1, n + 1), m)\n",
    "for p in possible:\n",
    "    print(' '.join(map(str, p)))"
   ]
  }
